import io/bufio;
from . import proto;

void proc_req(String cmd, char[][] args, bufio.Writer rsp_writer)
{
    if (cmd.eq("SET"))
    {
        do_cmd_set(args, rsp_writer);
        return;
    }

    if (cmd.eq("GET"))
    {
        do_cmd_get(args, rsp_writer);
        return;
    }

    send_err_rsp(rsp_writer, "不支持命令‘%s’".(cmd));
}
