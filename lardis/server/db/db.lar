import util;
from .. import data_types;

final long SLOT_COUNT = 1024L * 1024;

Slot[] slots;

void __init()
{
    slots = new Slot[SLOT_COUNT];
    for (long i = 0; i < SLOT_COUNT; ++ i)
    {
        slots[i] = new Slot();
    }
}

Slot slot_by_key(data_types.Str k)
{
    return slots[util.hash(k) % SLOT_COUNT];
}
